{
  "templateJs": {
    "prefix": "templateJs",
    "scope": "javascript",
    "body": ["Vue.component('${1:nome-componente}', {",
        " template: /*template*/ `",
        "   <div>${2:codigo}</div>`",
      "})"
    ],
    "description": "Cria template de componente base para vue em arquivo.js"
  },

  "data.js": {
    "prefix": "data.js",
    "scope": "javascript",
    "body": [
      "data: () => ({",
      " ${1:varName}: ${2:varValue}",
      "}),"
    ]
  }
}
