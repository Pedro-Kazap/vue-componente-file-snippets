{
  "templateJs": {
    "prefix": "templateJs",
    "scope": "javascript",
    "body": ["Vue.component('${1:nome-componente}', {",
        "\ttemplate: /*template*/ `",
        "\t\t<div>${2:codigo}</div>",
        "\t`",
      "})"
    ],
    "description": "Cria template de componente base para vue em arquivo.js"
  },

  "data.js": {
    "prefix": "dataJs",
    "scope": "javascript",
    "body": [
      "data: () => ({",
      "\t${1:varName}: ${2:varValue}",
      "})"
    ]
  }
}
