{
  "templateCoffee": {
    "prefix": "templateCoffee",
    "scope": "coffeescript",
    "body": ["Vue.component '${1:nome-componente}',",
        " template: /*template*/ `",
        "   <div>${2:codigo}</div>`"
    ],
    "description": "Cria template de componente base para vue em arquivo.js.coffe"
  },

  "data.coffee": {
    "prefix": "dataCoffee",
    "scope": "coffeescript",
    "body": [
      "data: ->",
      " {",
      "   ${1:varName}: ${2:varValue}",
      " }"
    ]
  }
}
